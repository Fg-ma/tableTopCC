{
  "compilerOptions": {
    "target": "esnext",
    "lib": ["ESNext", "dom"],
    "module": "esnext",
    "moduleResolution": "Node",
    "outDir": "./dashboard/dist",
    "rootDir": "./dashboard/src",
    "strict": true,
    "esModuleInterop": true,
    "allowSyntheticDefaultImports": true,
    "forceConsistentCasingInFileNames": true,
    "skipLibCheck": true,
    "types": ["node", "react"],
    "jsx": "react-jsx"
  },
  "include": [
    "./dashboard/src",
    "dashboard/src/serverRequests/ServerRequests.ts"
  ]
}
